{
  "name": "@concord-consortium/cloud-file-manager",
  "description": "Wrapper for providing file management for web applications",
  "author": "The Concord Consortium",
  "version": "2.1.0-pre.5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/cloud-file-manager.git"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  },
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "style": "./dist/css/app.css",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "^1.9.4",
    "@concord-consortium/token-service": "^2.1.0",
    "aws-sdk": "^2.1526.0",
    "base64-js": "^1.5.1",
    "classnames": "^2.4.0",
    "create-react-class": "^15.7.0",
    "diff": "^5.1.0",
    "iframe-phone": "^1.3.1",
    "jiff": "^0.7.3",
    "jquery": "^3.7.1",
    "js-base64": "^3.7.5",
    "js-sha256": "^0.10.1",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "pako": "^2.1.0",
    "query-string": "^7.1.3",
    "react-dom-factories": "^1.0.2",
    "screenfull": "^5.2.0",
    "svg-social-icons": "^1.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@types/create-react-class": "^15.6.6",
    "@types/jest": "^29.5.11",
    "@types/jiff": "^0.7.5",
    "@types/jquery": "^3.5.29",
    "@types/lodash": "^4.14.202",
    "@types/mime": "^3.0.4",
    "@types/node": "^16.18.68",
    "@types/pako": "^2.0.3",
    "@types/react": "^16.14.51",
    "@types/react-dom": "^16.9.22",
    "@types/react-dom-factories": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "cypress": "^7.7.0",
    "cypress-commands": "^1.1.0",
    "cypress-xpath": "^1.6.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "rimraf": "^5.0.5",
    "strip-json-comments-cli": "^3.0.0",
    "style-loader": "^3.3.3",
    "stylus": "^0.62.0",
    "stylus-loader": "^7.1.3",
    "sync-exec": "^0.6.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=16.14",
    "react-dom": ">=16.14"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && NODE_ENV=development webpack",
    "build:all": "npm run build:production && npm run build:library",
    "build:library": "./bin/build-library.sh",
    "build:production": "npm run clean && NODE_ENV=production webpack",
    "build:codap": "npm run clean && cross-env-shell NODE_ENV=production dest=../codap/apps/dg/resources/cloud-file-manager codap=1 noGlobals=1 noMap=1 'webpack && mv $dest/js/app.js $dest/js/app.js.ignore'",
    "build:codap:development": "npm run clean && cross-env-shell NODE_ENV=development dest=../codap/apps/dg/resources/cloud-file-manager codap=1 noGlobals=1 noMap=1 'webpack && mv $dest/js/app.js $dest/js/app.js.ignore'",
    "prepublishOnly": "npm run build:all",
    "publish:npm:latest": "npm publish --access public",
    "publish:npm:latest:preview": "npm publish --access public --dry-run",
    "publish:npm:next": "npm publish --access public --tag next",
    "publish:npm:next:preview": "npm publish --access public --tag next --dry-run",
    "yalc:publish": "npx yalc publish --push",
    "yalc:unpublish": "npx yalc installations clean @concord-consortium/cloud-file-manager",
    "strings:build": "strip-json-comments src/code/utils/lang/en-US-master.json > src/code/utils/lang/en-US.json",
    "strings:pull:usage": "echo Usage: `npm run strings:pull -- -a <poeditor_api_key>`",
    "strings:pull": "./bin/strings-pull-project.sh",
    "strings:push:usage": "echo Usage: `npm run strings:push -- -a <poeditor_api_key>`",
    "strings:push": "./bin/strings-push-project.sh",
    "start": "npm-run-all --parallel watch:server watch:build",
    "start:secure": "npm-run-all --parallel watch:server:secure watch:build",
    "watch:build": "webpack --watch --mode development",
    "watch:server": "live-server --host=localhost --port=8080 ./dist --open=examples",
    "watch:server:secure": "live-server --host=localhost --port=8080 --https=live-server-ssl-config.js ./dist --open=examples",
    "test:local": "cypress open --env testEnv=local",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint \"./src/code/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./src/code/**/*.{js,jsx,ts,tsx}\""
  }
}
